<Window x:Class="TcmsSimulator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TcmsSimulator"
        xmlns:bit="clr-namespace:TcmsSimulator.UIModel"
        xmlns:page="clr-namespace:TcmsSimulator.TabPage"
        mc:Ignorable="d"
        Title="MainWindow" Height="960" Width="1280">

    <Window.DataContext>
        <bit:StatusGridData/>
    </Window.DataContext>
    <Grid Margin="0,0,0,0">
        <TabControl Style="{StaticResource PageTabControlStyle}" x:Name="TCMS_ViewTab" HorizontalAlignment="Left" Height="auto" Margin="10,0,0,0" VerticalAlignment="Top" Width="auto"/>
    </Grid>
</Window>
